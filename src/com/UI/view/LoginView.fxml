<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="402.0" prefWidth="660.0" styleClass="scene" stylesheets="@com.UI.view.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.UI.controller.LoginViewController">
   <children>
      <TextField fx:id="username" alignment="CENTER" layoutX="171.0" layoutY="134.0" prefHeight="45.0" prefWidth="319.0" promptText="Type in your username">
         <font>
            <Font size="17.0" />
         </font>
      </TextField>
      <Label layoutX="296.0" layoutY="48.0" prefHeight="56.0" prefWidth="69.0" text="Login" textAlignment="CENTER">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <Button fx:id="confirmButton" layoutX="204.0" layoutY="299.0" mnemonicParsing="false" onKeyPressed="#loginEnter" prefHeight="45.0" prefWidth="92.0" styleClass="ComfirmButton" text="Comfirm" textFill="WHITE" />
      <Button fx:id="cancel" layoutX="365.0" layoutY="299.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="92.0" styleClass="CancelButton" text="Cancel" textFill="WHITE" />
      <PasswordField fx:id="password" alignment="CENTER" layoutX="171.0" layoutY="214.0" prefHeight="45.0" prefWidth="319.0" promptText="Type in your password">
         <font>
            <Font size="17.0" />
         </font>
      </PasswordField>
   </children>
</AnchorPane>
